<br/><br/>
<div class="container">
  <%= form_for [@client] do |f| %>
     <div class="form-group">
       <%= f.label :name %><br/>
       <%= f.text_field :name %>
    </div>
     <div class="form-group">
       <%= f.label :description %><br/>
       <%= f.text_area :description %>
    </div>
     <div class="form-group">
       <%= f.label :email %><br/>
       <%= f.text_field :email %>
    </div>
     <div class="form-group">
       <%= f.label :phone %><br/>
       <%= f.text_field :phone %>
    </div>
     <div class="form-group">
       <%= f.label :location %><br/>
       <%= f.text_field :location %>
    </div>
     <div class="form-group">
       <%= f.label :website %><br/>
       <%= f.text_field :website %>
    </div>



    <%= f.submit 'Submit', :class => "btn btn-success" %>
  <% end %>
</div>

